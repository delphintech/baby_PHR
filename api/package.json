{
  "name": "back",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./_bin/www",
    "dev": "nodemon ./_bin/www",
    "migrate": "node-pg-migrate up --migrations-dir ./_config/migration",
    "seed": "node ./_config/seed.js",
    "setup": "npm run migrate && npm run seed",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "csv-parse": "^6.1.0",
    "faker": "^6.6.6",
    "jest": "^30.2.0",
    "node-pg-migrate": "^8.0.3",
    "nodemon": "^3.1.11"
  }
}
